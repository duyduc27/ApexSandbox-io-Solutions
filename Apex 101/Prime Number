public Boolean isPrime(Integer num) {
    //code here
    if (num == 1 || num ==2) {
        return true;
    } else if( Math.mod(num,2) != 0 ){
        for (Integer i= 3; i< num ; i++) {
            if(Math.mod(num,i) ==0 ){
                
                return false;
            }
        }
    } else {
        return false;
    }
    
    return true;
}